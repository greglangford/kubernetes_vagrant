apiVersion: v1
clusters:
- cluster:
    certificate-authority: {{ kubernetes_pki_ca_cert }}
    server: {{ kubernetes_master_address }}
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: default
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: default
  user:
    client-certificate: {{ kubernetes_kubelet_client_cert }}
    client-key: {{ kubernetes_kubelet_client_cert_key }}
