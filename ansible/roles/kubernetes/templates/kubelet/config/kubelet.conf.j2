apiVersion: v1
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/pki/ca.pem
    server: https://192.168.255.10:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: default
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: default
  user:
    client-certificate: /etc/kubernetes/pki/kubelet-client.pem
    client-key: /etc/kubernetes/pki/kubelet-client-key.pem
